@model OfficeBoilerProject.OfficeAppService.Dto.OfficeDtoPut

@{
    ViewData["Title"] = "Add";
}

<h2>Add New Office</h2>
<hr />


<form method="post">
    <div class="form-group">
        <input type="number" name="Id" placeholder="Id" class="form-control" />
        <input type="text" name="Description" placeholder="Name" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">@("Add")</button>
</form>
