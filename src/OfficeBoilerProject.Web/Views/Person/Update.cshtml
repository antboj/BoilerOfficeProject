@model OfficeBoilerProject.PersonAppService.Dto.PersonDtoPut
@{
    ViewData["Title"] = "Update";
}

<h2>Update Person</h2>
<hr />

<form method="post">
    <div class="form-group">
        FirstName: <input type="text" name="FirstName" class="form-control" value="@Model.FirstName" />
        Lastname: <input type="text" name="LastName" class="form-control" value = "@Model.LastName" />
        Office: <input type="number" name="OfficeId" class="form-control" value = "@Model.OfficeId"/>
    </div>
    <button type="submit" class="btn btn-primary">@("Update")</button>
</form>

